page {
  padding-bottom: 98rpx;
}
.menu {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  display: flex;
  align-items: center;
  background-color: #F7F7FA;
  text-align: center;
  height: 98rpx;
}
.menu-item {
  flex: 1;
  color: #A5A5A5;
  padding: 60rpx 0 10rpx;
  font-size: 20rpx;
  background-position: center 10rpx;
  background-size: auto 40rpx;
  background-repeat: no-repeat;
}
.menu-item_active {
  color: #4987D8;
}
