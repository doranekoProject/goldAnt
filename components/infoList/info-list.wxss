/**index.wxss**/
.tab-hide {
  display: none;
}
.tab-list {
  font-family: Source Han Sans CN;
  position: relative;
  height: 180rpx;
  padding: 20rpx 20rpx 20rpx 208rpx;
  background: #fff;
  margin-bottom: 5rpx;
}
.tab-img {
  position: absolute;
  left: 20rpx;
  top: 20rpx;
  width: 160rpx;
  height: 160rpx;
}
.tab-left-info {
  float: left;
}
.tab-left-info .title {
  font-size: 32rpx;
  color: #262626;
  line-height: 54rpx;
}
.tab-left-info .address {
  font-size: 28rpx;
  color: #A5A5A5;
  line-height: 48rpx;
}
.tab-left-info .price-wrap {
  display: block;
  font-size: 24rpx;
  line-height: 41rpx;
  color: #A5A5A5;
  text-align: right;
}
.tab-left-info .price {
  color: #FE4B11;
}
.ads-list {
  font-family: Source Han Sans CN;
  padding: 20rpx 20rpx 20rpx 20px;
  background: #fff;
  height: 356rpx;
  margin-bottom: 20rpx;
}
.ads-list .ads-wrap {
  position: relative;
  height: 160rpx;
  padding-left: 188rpx;
}
.ads-list .state {
  color: #FE4B11;
  font-size: 24rpx;
  text-align: right;
  line-height: 41rpx;
  margin-bottom: 25rpx;
}
.ads-list .tab-img {
  left: 0;
  top: 0;
}
.button-warp {
  display: block;
  margin-top: 40rpx;
  text-align: right;
}
.button-warp .button {
  display: inline-block;
  color: #FE4B11;
  font-size: 24rpx;
  padding: 11rpx 24rpx;
  border:1rpx solid rgba(254,75,17,1);
  border-radius: 30rpx;
  background: #fff;
  width: auto;
}
.no-data {
  padding-top: calc(100% - 50%);
  text-align: center;
  color: #A5A5A5;
  font-size: 32rpx;
}
.order-other {
  position: fixed;
  bottom: 0;
  background: #fff;
  width: 100%;
  border-top: 1px solid #ccc;

}
.order-other_item {
  padding: 40rpx 20rpx;
  border-bottom: 1px solid #DBDBDB;
  display: flex;
  font-size: 32rpx;
  background: 99% center/ 30rpx auto url("data:image/png;base64,") no-repeat;
}
.order-other_item--active {
  background: 99% center/ 30rpx auto url("data:image/png;base64,") no-repeat;
}
.order-other_item:last-child {
  background: none !important;
}
.order-other_item--name {
  color: #A5A5A5;
}
.order-other_item--desc {
  color: #DBDBDB;
  margin-left: 50rpx;
}
.order-other_item--button {
  padding-left: 302rpx;
  font-size: 40rpx;
}
.order-other_item--button .order-other_item--name{
  color: #FE4B11;
}
.mask-wrap {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.mask {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.4;
}