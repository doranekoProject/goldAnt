.merchants-warp {
  font-family:Source Han Sans CN;
  padding: 236rpx 20rpx 0;
  position: relative;
}
.merchants-warp .img {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 279rpx;
}
.merchants-warp .form {
  position: relative;
  padding: 50rpx 56rpx 0;
  z-index: 1;
  height: 700rpx;
  background: rgba(255,255,255,1);
  border-radius:10px;
}
.form .title {
  margin-bottom: 54rpx;
  text-align: center;
  color: #545454;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 48rpx;
}
.section {
  position: relative;
  padding-left: 140rpx;
  height: 99rpx;
  line-height: 99rpx;
  border-bottom:1px solid rgba(219,219,219,1);
} 
.section-title {
  position: absolute;
  left: 0;
  top: 0;
  font-size: 28rpx;
  font-weight: 400;
  color:rgba(165,165,165,1);
}
.input{
  height: 99rpx;
  text-align: right;
  color: #A5A5A5;
  font-size: 30rpx;
}
.button {
  margin-top: 49rpx;
  height: 88rpx;
  background:#4987d8;
  line-height: 88rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight:400;
  color: rgba(255,255,255,1);
}