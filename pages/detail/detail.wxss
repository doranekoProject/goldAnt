/**detail.wxss**/
.detail-info {
  height:190rpx;
  padding: 30rpx 38rpx;
  box-sizing: paddding-box;
  background:rgba(255,255,255,1);
}
.detail-info_title {
  color: #262626;
  font-size: 40rpx;
}
.detail-info_desc {
  color: #A5A5A5;
  font-size: 30rpx;
  margin-top: 26rpx;
}
.detail-info_cost {
  font-size: 28rpx;
  margin-top: 36rpx;
  color: #FE4B11;
}
.detail-info_cost text {
  font-size: 40rpx;
}
.detail-type {
  margin-top: 10rpx;
  padding: 28rpx 38rpx;
  background:#fff;
  position: relative;
  font-size: 32rpx;
  min-height: 42rpx;
}
.detail-addr .detail-type_text{
  background-size: 39rpx 39rpx;
  background-image: url(data:image/png;base64,)
}
.detail-type_title {
  position: absolute;
  left: 38rpx;
  top: 26rpx;
  width: 90rpx;
  color: #A5A5A5;
}
.detail-type_text {
  padding-left: 100rpx;
  padding-right: 30rpx;
  background: right center/ 15rpx auto url("data:image/png;base64,")  no-repeat;
}
.detail-mian {
  color: #A5A5A5;
  margin: 36rpx 0 100rpx;
  border-top: 1px solid #DBDBDB;
}
.detail-mian_desc {
  padding-top: 20rpx;
}
.detail-mian_desc view {
  width: 100rpx;
  display: inline-block;
}
.detail-bot {
  position: fixed;
  left: 0;
  bottom: 0;
  background: #F7F7FA;
  width: 100%;
  height: 100rpx;
}
.detail-bot_right {
  position: fixed;
  right: 0;
  bottom: 0;
}
.detail-bot_btn {
  display: inline-block;
  height: 100rpx;
  padding: 0 50rpx;
  line-height: 100rpx;
  color: #fff;
  font-size: 32rpx;
}
.btn-orange {
  background: #FE4B11;
}
.btn-blue {
  background: #4987D8;
}
.home-bar_contact, .detail-favorite {
  display: inline-block;
  padding-top: 55rpx;
  font-size: 20rpx;
  color: #A5A5A5;
  width: 120rpx !important;
  text-align: center;
  line-height: 1;
}
.home-bar_contact {
  padding: 65rpx 0 0 !important;
  background: center 10rpx/50rpx 47rpx url("data:image/png;base64,") no-repeat;
}
.detail-favorite {
  padding-top: 65rpx;
  background: center 10rpx/50rpx 47rpx url("data:image/png;base64,") no-repeat; 
}
.selete-pop {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,0.3);
  z-index: 2;
}
.selete-pop_main {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background: #fff;
  padding-top: 28rpx;
}
.selete-pop_main--title {
  padding: 0 44rpx;
  font-size: 28rpx;
  color: #545454;
  pointer-events: retive;
}
.selete-pop_main--close {
  width: 42rpx;
  height: 42rpx;
  position: absolute;
  right: 44rpx;
  top: 30rpx;
  background: center center/ 100% url("data:image/png;base64,") no-repeat;
}
.selete-pop_main--row {
  padding: 0 44rpx;
  color: #A5A5A5;
  font-size: 32rpx;
  margin-top: 32rpx;
}
.selete-pop_main--name {
  width: 160rpx;
  display: inline-block;
  vertical-align: middle;
}
.selete-pop_main--item {
  display: inline-block;
  padding: 0 12rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  text-align: center;
  font-size: 28rpx;
  margin-left: 16rpx;
  vertical-align: middle;
  background-color: #A5A5A5;
}
.selete-pop_main--item_active {
  background: #4987D8;
}
.selete-pop_main--item_disable {
  pointer-events: none;
   opacity: 0.2;
}
.selete-pop_main--btn {
  background: #4987D8;
  text-align: center;
  height: 92rpx;
  width: 100%;
  line-height: 92rpx;
  color: #fff;
  font-size: 32rpx;
  margin-top: 64rpx;
}
.detail-desc {
  margin-top: 10rpx;
  padding: 24rpx 38rpx 100rpx;
  background-color: #fff;
}
.detail-desc_title {
  text-align: center;
  border-bottom: 1px solid #DBDBDB;
  margin-bottom: 20rpx;
}
.detail-desc_title text {
  display: inline-block;
  position: relative;
  padding-bottom: 24rpx;
  color: #262626;
  font-size: 32rpx;
}
.detail-desc_title text::after {
  content: "";
  position: absolute;
  left: 0;
  bottom: -1px;
  width: 100%;
  border-bottom: 1px  solid #FE4B11;
}
.selete-pop_main--reduce, .selete-pop_main--add {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 100% auto;
  vertical-align: middle;
}
.selete-pop_main--input {
  display: inline-block;
  width: 40rpx;
  text-align: center;
  vertical-align: middle;
  margin: 0 20rpx;
}
.selete-pop_main--add {
  background-image: url("data:image/png;base64,");
}
.selete-pop_main--reduce {
  background-image: url("data:image/png;base64,");
}
.selete-pop_main--bot {
  display: flex;
}
.selete-pop_main--btncat {
  background-color: #FE4B11;
}