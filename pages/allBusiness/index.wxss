/**all business.wxss**/
.business-list {
  margin-top: 10rpx;
  background: #fff;
}
.business-list_item {
  padding: 20rpx 20rpx 0;
  display: flex;
}
.business-list_item--img {
  width: 200rpx;
  height: 200rpx;
}
.business-list_info {
  flex: 1;
  padding-right: 32rpx;
  margin-left: 40rpx;
  font-size: 28rpx;
  color: #FE4B11;
  border-bottom: 1px solid #DBDBDB;
  padding-bottom: 20rpx;
}
.business-list_item:last-child .business-list_info{
  border: 0;
}
.business-list_title {
  color: #262626;
  font-size: 36rpx;
  margin-bottom: 18rpx;
}
.business-list_desc {
  font-size: 28rpx;
  color: #A5A5A5;
  height: 56rpx;
  overflow: hidden;
}
.business-list_cost {
  font-size: 38rpx;
}
.business-list_bot {
  position: relative;
  margin-top: 30rpx;
}
.business-list_bot--cart {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 47rpx;
  height: 43rpx;
  background: left center / 100% auto url("data:image/png;base64,") no-repeat;
}