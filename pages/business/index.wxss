/**homepage.wxss**/
page {
  padding-bottom: 0 !important;
}
.no-data {
  padding: 100rpx;
  color: #A5A5A5;
  font-size: 24rpx;
  text-align: center;
}
.shop-banner {
  height: 360rpx;
}
.swiper-item_img {
  width: 100%;
  height: 360rpx;
}
.home {
  overflow: hidden;
}
.home-bar {
  position: relative;
  width:100%;
  height:68rpx;
  padding: 10rpx 0;
  background:rgba(73,135,216,1);
  color: #fff;
  font-size: 30rpx;
  z-index: 3;
}
.home-bar_area {
  position: absolute;
  left: 0;
  top: 30rpx;
  padding: 0 30rpx 0 38rpx;
}
.home-bar_area::after {
  content: "";
  position: absolute;
  right: 0;
  top: 16rpx;
  border-width: 10rpx;
  border-color: #fff transparent transparent;
  border-style: solid;
}
.picker {
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 100rpx;
  overflow: hidden;
}
.home-bar_seach {
  margin: 0 120rpx 0 180rpx;
  height:68rpx;
  border-radius:8px;
  padding-left: 70rpx;
  background: #fff 20rpx center/32rpx auto url('data:image/png;base64,') no-repeat;
}
.home-bar_seach input {
  width: 100%;
  height: 100%;
  line-height: 100%;
  color: #262626;
}
.placeholder {
  color: #D1D1D1;
}
.home-bar_contact {
  position: absolute;
  right: 35rpx;
  top: 25rpx;
  padding: 0;
  width: 50rpx !important;
  height: 47rpx;
  background: left center/ 100% auto url("data:image/png;base64,") no-repeat;
}
.home-filter {
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  height:100rpx;
  background:rgba(255,255,255,1);
  color: #262626;
  font-size: 30rpx;
  position: relative;
  z-index: 3;
}
.home-filter_item {
  flex: 1;
}
.home-filter_item text{
  display: inline-block;
  position: relative;
  padding-right: 30rpx;
}
.home-filter_item text::after {
  content: "";
  position: absolute;
  right: 0;
  top: 16rpx;
  border-width: 10rpx;
  border-color: #262626 transparent transparent;
  border-style: solid;
  transition: .2s transform;
}
.home-filter_item--active {
  color: #FE4B11;
}
.home-filter_item--active text::after  {
  top: 8rpx;
  border-top-color: #FE4B11;
  border-style: solid;
  transform: rotate(180deg);
}
.home-list {
  margin-top: 10rpx;
  background: #fff;
}

.list-col {
  padding-bottom: 20rpx;
}
.list-col .home-list_item{
  width: 50%;
  font-size:none;
  display: inline-block;
  vertical-align: top;
}
.list-col .home-list_item--img {
  margin: 20rpx;
  width: 345rpx;
  height: 345rpx;
}
.list-col .home-list_info {
  margin:  0 20rpx;
  font-size: 28rpx;
  color: #FE4B11;
}
.home-list_item:last-child .home-list_info{
  border: 0;
}
.list-col  .home-list_title {
  color: #262626;
  font-size: 32rpx;
  margin-bottom: 18rpx;
  height: 86rpx;
  overflow: hidden;
}
.home-list_desc {
  font-size: 28rpx;
  color: #A5A5A5;
  height: 56rpx;
  overflow: hidden;
}
.home-list_cost {
  font-size: 38rpx;
}
.home-list_bot {
  position: relative;
  margin-top: 30rpx;
  height: 28rpx;
  font-size: 28rpx;
  color: #FE4B11;
}
.drop-down {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.drop-down_layer {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,0.3);
}
.drop-down_box {
  position: fixed;
  left: 0;
  top: 186rpx;
  width: 100%;
  border-top: 1px solid #DBDBDB;
  background: #fff;
  z-index: 2;
  min-height: 506rpx;
  color: #545454;
  font-size: 36rpx;
}
.drop-down_box--active {
  background: #fff;
  color: #FE4B11;
}
.drop-down_col .drop-down_box--col:nth-child(1) {
  background: #F6F6F6;
}
.drop-down_col  {
    display: flex;
}
.drop-down_bot > view {
  margin-top: 32rpx;
}
.drop-down_box--item {
  padding: 20rpx 0;
  margin: 0 38rpx;
  border-bottom: 1px solid #DBDBDB;
}
.drop-down_col .drop-down_box--item {
  padding: 28rpx 38rpx;
  border: 0;
  margin: 0;
}
.drop-down_bot {
  margin:  0 38rpx;
  font-size: 28rpx;
  color: #A5A5A5;
}
.drop-down_bot--name {
  width: 160rpx;
  display: inline-block;
  vertical-align: middle;
}
.drop-down_bot--item {
  display: inline-block;
  width:108rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #A5A5A5;
  color: #fff;
  text-align: center;
  font-size: 28rpx;
  margin-left: 16rpx;
  vertical-align: middle;
}
.drop-down_bot--item_active {
  background: #4987D8;
}
.drop-down_bot--btn {
  background: #4987D8;
  text-align: center;
  height: 92rpx;
  width: 678rpx;
  line-height: 92rpx;
  color: #fff;
  font-size: 32rpx;
  margin-bottom: 20rpx;
}
.drop-down_bot--reduce, .drop-down_bot--add {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 100% auto;
  vertical-align: middle;
}
.drop-down_bot--input {
  display: inline-block;
  width: 40rpx;
  text-align: center;
  vertical-align: middle;
}
.drop-down_bot--add {
  background-image: url("data:image/png;base64,");
}
.drop-down_bot--reduce {
  background-image: url("data:image/png;base64,");
}
.drop-down_buy .drop-down_box--item {
background: right center/ 30rpx auto url("data:image/png;base64,") no-repeat;
}
.drop-down_buy .drop-down_box--active {
  background: right center/ 30rpx auto url("data:image/png;base64,") no-repeat;
}