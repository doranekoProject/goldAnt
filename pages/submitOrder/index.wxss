/**submit order.wxss**/
.placeholder {
  color: #DBDBDB;
}
.order-info {
  background: #fff;
  display: flex;
  padding: 40rpx;
}
.order-info_box--title {
  color: #262626;
  font-size: 36rpx;
  margin-bottom: 18rpx;
  height: 146rpx;
}
.order-info_box--cost {
  font-size: 38rpx;
}
.order-info_img {
  width: 200rpx;
  height: 200rpx;
}
.order-info_box {
  flex: 1;
  padding-right: 32rpx;
  margin-left: 40rpx;
  font-size: 28rpx;
  color: #FE4B11;
}
.order-other {
  border-top: 1px solid #DBDBDB;
  background: #fff;
  padding: 0 40rpx 60rpx;
}
.order-other_item {
  padding: 40rpx 0;
  border-bottom: 1px solid #DBDBDB;
  display: flex;
  font-size: 32rpx;
  background: right center/ 30rpx auto url("data:image/png;base64,") no-repeat;
}
.order-other_item--active {
  background: right center/ 30rpx auto url("data:image/png;base64,") no-repeat;
}
.order-other_item:last-child {
  background: none !important;
}
.order-other_item--name {
  color: #A5A5A5;
}
.order-other_item--desc {
  color: #DBDBDB;
  margin-left: 50rpx;
}
.order-info_box--bot {
  position: relative;
}
.order-info_box--quantity {
  position: absolute;
  right: 0;
  top: 0;
}
.order-info_box--reduce, .order-info_box--add {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 100% auto;
  vertical-align: middle;
}
.order-info_box--input {
  display: inline-block;
  width: 40rpx;
  text-align: center;
  vertical-align: middle;
  color: #545454;
}
.order-info_box--add {
  background-image: url("data:image/png;base64,");
}
.order-info_box--reduce {
  background-image: url("data:image/png;base64,");
}
.order-bot {
  position: fixed;
  left: 0;
  bottom: 0;
  background: #F7F7FA;
  width: 100%;
  height: 100rpx;
  font-size: 28rpx;
  color: #A5A5A5;
}
.order-bot_right {
  position: fixed;
  right: 0;
  bottom: 0;
}
.order-bot_btn {
  display: inline-block;
  height: 100rpx;
  padding: 0 50rpx;
  line-height: 100rpx;
  color: #fff;
  font-size: 32rpx;
}
.btn-blue {
  background: #4987D8;
}
.order-bot_cost {
  display: inline-block;
  margin-right: 24rpx;
  color: #FE4B11;
}
.add-address {
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  background-color: #fff;
  height: 243rpx;
  margin-bottom: 20rpx;
  font-size: 28rpx;
  color: #A5A5A5;
}
.address-box {
  display: flex;
  align-items: center;
  background-color: #fff;
  height: 193rpx;
  font-size: 28rpx;
  color: #A5A5A5;
  line-height: 50rpx;
  padding: 25rpx;
}
.add-address_add {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 100% auto;
  vertical-align: middle;
  margin-top: 30rpx;
}
.add-address_add {
  background-image: url("data:image/png;base64,");
}
.address-list .label {
  padding-right: 5rpx;
}